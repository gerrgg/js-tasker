ker\", token);\n    }\n  }, [loginResult.data, setToken]);\n\n  return (\n    <Wrapper>\n      {page === \"login\" ? (\n        <LoginForm\n          setPage={setPage}\n          loginUser={loginUser}\n          username={username}\n          password={password}\n        />\n      ) : (\n        <RegisterForm\n          setPage={setPage}\n          registerUser={registerUser}\n          username={username}\n          password={password}\n        />\n      )}\n    </Wrapper>\n  );\n};\n\nconst Wrapper = styled.div`\n  padding: 0 1rem;\n  margin: auto;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n`;\n\nexport default Login;\n","/home/gerrg/projects/js-tasker/client/src/components/Loader.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Spacer.js",[],"/home/gerrg/projects/js-tasker/client/src/hooks/useField.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Form.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Heading.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Flex.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Button.js",[],"/home/gerrg/projects/js-tasker/client/src/components/TextLink.js",[],"/home/gerrg/projects/js-tasker/client/src/components/Login.js",[],"/home/gerrg/projects/js-tasker/client/src/queries/user.js",[],"/home/gerrg/projects/js-tasker/client/src/pages/Dashboard.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":16,"column":20,"nodeType":"77","messageId":"78","endLine":16,"endColumn":34},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"no-unused-vars","'registerResult' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]